{
  "name": "doubleyong",
  "description": "第一个chrome extensions",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
    "16": "icons/logo.png",
    "48": "icons/logo.png",
    "128": "icons/logo.png"
  },
  "action": {
    "default_title": "doubleyong的",
    "default_popup": "popup.html"
  },
  "permissions": ["storage", "notifications", "tabs", "history"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/vendor.js"]
    }
  ],
  "host_permissions": ["<all_urls>"],
  "options_page": "options.html",
  "background": {}
}
